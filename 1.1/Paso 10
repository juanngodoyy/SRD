# Let NetworkManager manage all devices on this system
network:
  version: 2
  renderer: networkd
  ethernets:
   enp0s3:
    dhcp4: false
    addresses: [192.168.5.15/24]
    routes:
     - to: default
       via: 192.168.5.100
    nameservers:
     search: [juangg.edu]
     addresses: [192.168.5.200, 8.8.8.8]
